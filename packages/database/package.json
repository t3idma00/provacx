{
  "name": "@provacx/database",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "default": "./src/client.ts"
    }
  },
  "scripts": {
    "postinstall": "node ./scripts/prisma-generate-with-retry.mjs",
    "db:generate": "node ./scripts/prisma-generate-with-retry.mjs",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "lint": "eslint src --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0"
  },
  "devDependencies": {
    "@provacx/eslint-config": "workspace:*",
    "@provacx/typescript-config": "workspace:*",
    "@types/node": "^20.12.12",
    "eslint": "^8.57.0",
    "prisma": "^5.14.0",
    "tsx": "^4.10.0",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
